{
  "name": "zani",
  "version": "0.0.14",
  "description": "",
  "main": "index.js",
  "scripts": {
    "offline:dev": "bnr offline:dev",
    "invoke:notifier": "bnr invoke:notifier",
    "deploy:prod": "bnr deploy:prod",
    "deploy:test": "bnr deploy:test",
    "lint": "eslint './src/**/*.ts'"
  },
  "betterScripts": {
    "offline:dev": {
      "command": "sls offline --stage development",
      "env": {
        "NODE_ENV": "development"
      }
    },
    "invoke:notifier": {
      "command": "sls invoke local -f notifier --stage development",
      "env": {
        "NODE_ENV": "development"
      }
    },
    "deploy:prod": {
      "command": "sls deploy --stage production",
      "env": {
        "NODE_ENV": "production"
      }
    },
    "deploy:test": {
      "command": "sls deploy --stage test",
      "env": {
        "NODE_ENV": "test"
      }
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ClaudeSeo/zani.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/ClaudeSeo/zani/issues"
  },
  "homepage": "https://github.com/ClaudeSeo/zani#readme",
  "devDependencies": {
    "@types/moment": "^2.13.0",
    "@types/node": "^12.12.21",
    "@types/uuid": "^3.4.6",
    "@types/winston": "^2.4.4",
    "@typescript-eslint/eslint-plugin": "^2.12.0",
    "@typescript-eslint/parser": "^2.12.0",
    "better-npm-run": "^0.1.1",
    "eslint": "^6.6.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-plugin-import": "^2.19.1",
    "serverless": "^1.59.3",
    "serverless-offline": "^5.12.1",
    "serverless-webpack": "^5.3.1",
    "ts-loader": "^6.2.1",
    "typescript": "^3.7.4",
    "webpack": "^4.41.4"
  },
  "dependencies": {
    "aws-sdk": "^2.567.0",
    "axios": "^0.19.0",
    "d2p": "https://cdn.engn.me/npm/d2p.tar.gz",
    "moment": "^2.24.0",
    "uuid": "^3.3.3",
    "winston": "^3.2.1"
  }
}
